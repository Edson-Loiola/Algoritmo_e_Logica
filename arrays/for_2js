

const notas = [7,8,9,5,1,8,8,6];

let valores = '';

//foreach para percorrer o array
for(let nota of notas){  //of para acesar os valores do array
    console.log(nota);
}

//imprimir todas as notas
for(let nota of notas){
    valores += nota + ' ';
}
console.log(valores + 'Valores');


let inidices = '';
for(let indice in notas){  //in para acesar os indices do array
    inidices += indice + ' ';
}
console.log(inidices + 'Indices');